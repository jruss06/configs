! special
*.foreground:   #8a8986
*.background:   #28211c
*.cursorColor:  #8a8986

! black
*.color0:       #28211c
*.color8:       #666666

! red
*.color1:       #99472f
*.color9:       #e27554

! green
*.color2:       #54be0d
*.color10:      #54be0d

! yellow
*.color3:       #f9ee98
*.color11:      #c0b669

! blue
