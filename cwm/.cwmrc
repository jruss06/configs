borderwidth 2
color activeborder purple
color inactiveborder black
gap 0 0 0 0
color menubg '#191919'
color menufg '#ffffff'
color font '#DEDEDE'
fontname terminus
snapdist 10

bind CM-Return urxvt
bind 4-x delete
bind 4-Return urxvt
bind 4-p dmenu_run
bind 4-1 group1
bind 4-2 group2
bind 4-3 group3
bind 4-4 group4
bind 4-5 group5
bind 4-6 group6

mousebind 4-1 window_move
mousebind 4-3 window_resize

ignore bar
ignore Tint2
sticky yes
